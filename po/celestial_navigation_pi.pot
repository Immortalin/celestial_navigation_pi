#: src/Sight.cpp:561 src/Sight.cpp:699
msgid ""
"Formulas used to calculate sight\n"
"\n"
msgstr ""

#: src/Sight.cpp:565 src/Sight.cpp:703
#, c-format
msgid ""
"Index Error is %.4f degrees\n"
"\n"
msgstr ""

#: src/Sight.cpp:570 src/Sight.cpp:708
#, c-format
msgid ""
"Eye Height is %.4f meters\n"
"Height Correction Degrees = 1.758*sqrt(%.4f) / 60.0\n"
"Height Correction Degrees = %.4f\n"
msgstr ""

#: src/Sight.cpp:577 src/Sight.cpp:787
#, c-format
msgid ""
"\n"
"Apparent Altitude (Ha)\n"
"ApparentAltitude = Measurement - IndexCorrection - EyeHeightCorrection\n"
"ApparentAltitude = %.4f - %.4f - %.4f\n"
"ApparentAltitude = %.4f\n"
msgstr ""

#: src/Sight.cpp:595 src/Sight.cpp:798
#, c-format
msgid ""
"\n"
"Refraction Correction\n"
"x = tan(Pi/180*ApparentAltitude + 4.848e-2*(Pi/180) / "
"(tan(Pi/180*ApparentAltitude) + .028))\n"
"x = tan(Pi/180*%.4f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.4f) + .028))\n"
"x = %.4f\n"
msgstr ""

#: src/Sight.cpp:600 src/Sight.cpp:803
#, c-format
msgid ""
"RefractionCorrection = .267 * Pressure / (x*(Temperature + 273.15)) / 60.0\n"
"RefractionCorrection = .267 * %.4f / (x*(%.4f + 273.15)) / 60.0\n"
"RefractionCorrection = %.4f\n"
msgstr ""

#: src/Sight.cpp:615 src/Sight.cpp:817
#, c-format
msgid ""
"\n"
"Sun selected, Limb Correction\n"
"ra = %.4f, lc = 0.266564/ra = %.4f\n"
msgstr ""

#: src/Sight.cpp:625 src/Sight.cpp:741
#, c-format
msgid ""
"\n"
"Moon selected, Limb Correction\n"
"SD = %.4f\n"
"lc = 180/Pi * asin(Pi/180*SD)\n"
"lc = %.4f\n"
msgstr ""

#: src/Sight.cpp:635 src/Sight.cpp:750 src/Sight.cpp:825
msgid "Upper Limb"
msgstr ""

#: src/Sight.cpp:638 src/Sight.cpp:753 src/Sight.cpp:828
msgid "Lower Limb"
msgstr ""

#: src/Sight.cpp:641 src/Sight.cpp:831
#, c-format
msgid ""
"\n"
"LimbCorrection = %.4f\n"
msgstr ""

#: src/Sight.cpp:645 src/Sight.cpp:835
#, c-format
msgid ""
"\n"
"Corrected Altitude\n"
"CorrectedAltitude = ApparentAltitude - RefractionCorrection - "
"LimbCorrection\n"
"CorrectedAltitude = %.4f - %.4f - %.4f\n"
"CorrectedAltitude = %.4f\n"
msgstr ""

#: src/Sight.cpp:659 src/Sight.cpp:849
#, c-format
msgid ""
"\n"
"Sun selected, parallax correction\n"
"rad = %.4f, HP = 0.002442/rad = %.4f\n"
msgstr ""

#: src/Sight.cpp:668 src/Sight.cpp:773
#, c-format
msgid ""
"\n"
"Moon selected, parallax correction\n"
"HP = %.4f\n"
msgstr ""

#: src/Sight.cpp:674 src/Sight.cpp:855
#, c-format
msgid ""
"ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * "
"CorrectedAltitude))\n"
"ParallaxCorrection = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * "
"%.4f))\n"
"ParallaxCorrection = %.4f\n"
msgstr ""

#: src/Sight.cpp:681
#, c-format
msgid ""
"\n"
"Observed Altitude (Ho)\n"
"ObservedAltitude = CorrectedAltitude - ParallaxCorrection\n"
"ObservedAltitude = %.4f - %.4f\n"
"ObservedAltitude = %.4f\n"
msgstr ""

#: src/Sight.cpp:715
#, c-format
msgid ""
"\n"
"Apparent Moon Altitude (Ha)\n"
"ApparentAltitudeMoon = MeasurementMoon - IndexCorrection - "
"EyeHeightCorrection\n"
"ApparentAltitudeMoon = %.4f - %.4f - %.4f\n"
"ApparentAltitudeMoon = %.4f\n"
msgstr ""

#: src/Sight.cpp:726
#, c-format
msgid ""
"\n"
"Refraction Correction\n"
"x = tan(Pi/180*ApparentAltitudeMoon + 4.848e-2*(Pi/180) / "
"(tan(Pi/180*ApparentAltitudeMoon) + .028))\n"
"x = tan(Pi/180*%.4f + 4.848e-2*(Pi/180) / (tan(Pi/180*%.4f) + .028))\n"
"x = %.4f\n"
msgstr ""

#: src/Sight.cpp:731
#, c-format
msgid ""
"RefractionCorrectionMoon = .267 * Pressure / (x*(Temperature + 273.15)) / "
"60.0\n"
"RefractionCorrectionMoon = .267 * %.4f / (x*(%.4f + 273.15)) / 60.0\n"
"RefractionCorrectionMoon = %.4f\n"
msgstr ""

#: src/Sight.cpp:756 src/Sight.cpp:759
#, c-format
msgid ""
"\n"
"LimbCorrectionMoon = %.4f\n"
msgstr ""

#: src/Sight.cpp:762
#, c-format
msgid ""
"\n"
"Corrected Altitude\n"
"CorrectedAltitudeMoon = ApparentAltitudeMoon - RefractionCorrectionMoon - "
"LimbCorrectionMoon\n"
"CorrectedAltitudeMoon = %.4f - %.4f - %.4f\n"
"CorrectedAltitudeMoon = %.4f\n"
msgstr ""

#: src/Sight.cpp:777
#, c-format
msgid ""
"ParallaxCorrectionMoon = -180/Pi * asin( sin(Pi/180 * HP ) * cos(Pi/180 * "
"CorrectedAltitude))\n"
"ParallaxCorrectionMoon = -180/Pi * asin( sin(Pi/180 * %.4f ) * cos(Pi/180 * "
"%.4f))\n"
"ParallaxCorrectionMoon = %.4f\n"
msgstr ""

#: src/Sight.cpp:866
#, c-format
msgid ""
"CorrectionsMoon = %.4f\n"
"CorrectionsBody = %.4f\n"
"Corrections = abs(CorrectionsMoon - CorrectionsBody) = %.4f\n"
msgstr ""

#: src/Sight.cpp:872
#, c-format
msgid ""
"CorrectedMeasurement = Measurement - Corrections - IndexCorrection\n"
"CorrectedMeasurement = %.4f - %.4f - %.4f\n"
"CorrectedMeasurement = %.4f\n"
msgstr ""

